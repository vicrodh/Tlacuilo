{
  "$schema": "https://schema.tauri.app/config/1",
  "productName": "ihpdf",
  "version": "0.1.0",
  "identifier": "com.ihpdf.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "I H PDF",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
