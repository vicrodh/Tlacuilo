{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "dialog:allow-message",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-copy-file",
    "fs:read-all",
    "fs:write-all",
    "store:default"
  ]
}
